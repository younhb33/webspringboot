<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.yoon.mapper">

<insert id="save_member" parameterType="kr.co.yoon.membership_DTO">
insert into MEMBERSHIP
values (MNO.nextval,#{MCODE},#{MID},#{MNAME},#{MNICK},#{MPASS},#{MEMAIL},#{MHP},#{MJOIN},SYSDATE)
</insert>

<select id="alldata" resultType="kr.co.yoon.membership_DTO">
select * from MEMBERSHIP
</select>

<!-- 하나의 데이터만 출력 -->
<select id="onedata" resultType="kr.co.yoon.membership_DTO">
select * from MEMBERSHIP where MID = #{MID}
</select>

</mapper>